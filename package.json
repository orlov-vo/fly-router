{
  "name": "fly-router",
  "version": "0.0.0",
  "description": "Tiny router with middleware-based archeture",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf api.md coverage/",
    "build": "rollup --config",
    "api": "documentation build src/*.js -f md -o api.md",
    "spell": "npm run api && yaspeller *.md",
    "lint": "eslint src/*.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest --coverage && npm run lint && npm run build && size-limit && npm run typecheck && npm run spell"
  },
  "keywords": [
    "router",
    "middleware"
  ],
  "author": "Vladislav Orlov <me@orlov-vo.ru>",
  "license": "BSD-3-Clause",
  "repository": "orlov-vo/fly-router",
  "lint-staged": {
    "*.md": "yaspeller",
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "size-limit": [
    {
      "path": "dist/fly-router.js",
      "limit": "2.35 KB",
      "running": false
    },
    {
      "path": "dist/fly-router.mjs",
      "limit": "2.26 KB",
      "running": false
    }
  ],
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "ignoreText": [
      " \\(by [^)]+\\)."
    ]
  },
  "dependencies": {
    "core-js": "^3.1.3"
  },
  "peerDependencies": {
    "core-js": ">= 3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-async-to-promises": "^0.8.11",
    "documentation": "^11.0.0",
    "eslint": "^5.16.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.13.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "size-limit": "^1.3.5",
    "typescript": "^3.5.1",
    "yaspeller": "^5.1.0"
  }
}
